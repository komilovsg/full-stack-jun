# Telegram Bot Token
# Получить можно у @BotFather в Telegram
# Пример: 1234567890:ABCdefGHIjklMNOpqrsTUVwxyz
BOT_TOKEN=your_bot_token_here

# PostgreSQL настройки
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=telegram_bot
POSTGRES_PORT=5432

# Redis настройки
REDIS_PORT=6379

# Gemini API Key
# Получить можно на https://aistudio.google.com/app/apikey
# Это бесплатный API от Google для работы с LLM
GEMINI_API_KEY=your_gemini_api_key_here

# DeepSeek API Key (опционально)
# Альтернативный LLM API для анализа
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_URL=https://api.deepseek.com

# Alibaba Model Studio (Qwen) API Key (опционально)
# OpenAI-совместимый API от Alibaba Cloud
DASHSCOPE_API_KEY=your_dashscope_api_key_here
DASHSCOPE_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1
DASHSCOPE_MODEL=qwen-plus

# Время жизни кэша в секундах (по умолчанию 20 минут = 1200 секунд)
CACHE_TTL=1200

# Порт для веб-интерфейса
WEB_PORT=3000
